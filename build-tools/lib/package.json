{
  "name": "luplink",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "start": "ng serve",
    "build": "ng build",
    "build-standalone": "ng build --project=luplink-app",
    "build-luplink-debug": "ng build --project=luplink-app --configuration debug",
    "test": "ng test luplink-app --no-watch && ng test ngx-luplink --no-watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pretest": "echo 'killing existing API servers'; if lsof -t -i:8001; then sudo kill $(sudo lsof -t -i:8001); fi; pwd; echo 'Starting Backend for integration tests'; python3 ../API/LinkBudgetAnalysisREST.py &",
    "posttest": "sudo kill $(sudo lsof -t -i:8001)",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "tu": "ng test -c unit --browsers=ChromeHeadless",
    "preti": "echo 'killing existing API servers';if lsof -t -i:8001; then sudo kill $(sudo lsof -t -i:8001); fi; pwd; sudo kill $(sudo lsof -t -i:8001); echo 'Starting Backend for integration tests'; python3 ../API/LinkBudgetAnalysisREST.py &",
    "ti": "ng test -c integration --browsers=ChromeHeadless",
    "postti": "sudo kill $(sudo lsof -t -i:8001)",
    "preth": "echo 'killing existing API servers'; if lsof -t -i:8001; then sudo kill $(sudo lsof -t -i:8001); fi; pwd; echo 'Starting Backend for integration tests'; python3 ../API/LinkBudgetAnalysisREST.py &",
    "th": "ng test --watch=false --browsers=ChromeHeadless",
    "postth": "python3 ../API/test_API.py",
    "preversion": "npm run test-headless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/common": "~11.2.1",
    "@angular/compiler": "~11.2.1",
    "@angular/core": "~11.2.1",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "~11.2.1",
    "@angular/platform-browser-dynamic": "~11.2.1",
    "@angular/router": "~11.2.1",
    "@creativebulma/bulma-tooltip": "^1.2.0",
    "@fortawesome/angular-fontawesome": "^0.9.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "bootstrap": "^5.0.2",
    "bulma": "^0.9.3",
    "bulma-o-steps": "^1.1.0",
    "d3": "^7.0.0",
    "ng-katex": "^2.0.3",
    "ngx-toastr": "^14.0.0",
    "rxjs": "~6.6.0",
    "sanitize-filename": "^1.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular-eslint/builder": "4.3.0",
    "@angular-eslint/eslint-plugin": "4.3.0",
    "@angular-eslint/eslint-plugin-template": "4.3.0",
    "@angular-eslint/schematics": "4.3.0",
    "@angular-eslint/template-parser": "4.3.0",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "~11.2.1",
    "@types/d3": "^7.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.16",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "karma-verbose-reporter": "0.0.6",
    "ng-mocks": "^12.3.1",
    "ng-packagr": "~11.1.4",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.2"
  }
}
